{
  "transforms": [
    {
      "mode": "sql",
      "include": "**/*.sql",
      "emitTemplate": "{{dir}}/{{name}}.queries.ts"
    },
    {
      "mode": "ts",
      "include": "**/*.ts",
      "emitTemplate": "{{dir}}/{{name}}.types.ts"
    },
    {
      "mode": "ts-implicit",
      "include": "**/*.ts",
      "functionName": "sql",
      "emitFileName": "sql/index.ts"
    }
  ],
  "typesOverrides": {
    "date": {
      "return": "string"
    },
    "int8": "BigInt",
    "category": { "return": "./src/customTypes#Category" }
  },
  "srcDir": "./src/",
  "dbUrl": "postgres://postgres:password@localhost/postgres"
}
